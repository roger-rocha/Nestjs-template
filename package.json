{
  "name": "wpix",
  "version": "0.0.1",
  "description": "WPIX API - NestJS + Prisma",
  "author": "Bertin",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=development nest start",
    "start:hml": "NODE_ENV=HML nest start --watch",
    "start:dev": "NODE_ENV=DEV nest start --watch",
    "start:debug": "NODE_ENV=DEV nest start --debug --watch",
    "start:prod": "NODE_ENV=DEV node dist/main",
    "test": "NODE_ENV=TEST -- vitest --run",
    "test:ui": "NODE_ENV=TEST vitest --ui --threads false --coverage.enabled",
    "test:coverage": "NODE_ENV=TEST vitest run --coverage",
    "prisma:push:test": "NODE_ENV=TEST npx prisma db push",
    "prisma:push:dev": "NODE_ENV=DEV npx prisma db push",
    "prisma:push:prod": "NODE_ENV=PROD npx prisma db push",
    "prisma:push:hml": "NODE_ENV=HML npx prisma db push",
    "prisma:push:*": "npm run prisma:push:dev; npm run prisma:push:test; npm run prisma:push:prod; npm run prisma:push:hml",
    "prisma:pull:test": "NODE_ENV=TEST npx prisma db pull",
    "prisma:pull:dev": "NODE_ENV=DEV npx prisma db pull",
    "prisma:pull:prod": "NODE_ENV=DEV npx prisma db pull",
    "prisma:pull:hml": "NODE_ENV=HML npx prisma db pull",
    "prisma:generate:test": "NODE_ENV=TEST npx prisma generate",
    "prisma:generate:dev": "NODE_ENV=DEV npx prisma generate"
  },
  "dependencies": {
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/swagger": "^7.1.8",
    "@prisma/client": "^5.1.1",
    "@vitest/ui": "^0.31.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "express-basic-auth": "^1.2.1",
    "md5": "^2.3.0",
    "nestjs-prisma": "^0.22.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "ts-toolbelt": "^9.6.0",
    "uuid": "^9.0.0",
    "webpack": "^5.88.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "^10.1.12",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.1.3",
    "@types/express": "^4.17.17",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.18",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@vitest/coverage-v8": "^0.34.2",
    "@vitest/ui": "^0.34.2",
    "husky": "^8.0.3",
    "prettier": "^3.0.2",
    "prisma": "^5.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "^5.1.6",
    "vitest": "^0.34.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
